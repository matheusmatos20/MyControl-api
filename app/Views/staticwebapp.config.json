{
  "routes": [
    { "route": "/", "redirect": "/Index/index.html", "statusCode": 302 },
    { "route": "/index.html", "rewrite": "/Index/index.html" }
  ],
  "navigationFallback": {
    "rewrite": "/Index/index.html",
    "exclude": [
      "/api/*",
      "/favicon.ico",
      "/robots.txt",
      "/sitemap.xml"
    ]
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=60"
  }
}

